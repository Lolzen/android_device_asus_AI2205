/dev/gaming_input[0-9]*                                                         u:object_r:asus_gaming_device:s0

# AeroActive Cooler 7
/dev/asus_cooler                                                               u:object_r:asus_cooler_device:s0
/dev/cooler_fan                                                                u:object_r:asus_cooler_device:s0
/dev/cooler_temp                                                               u:object_r:asus_cooler_device:s0

# AURA Sync RGB System
/dev/aura_sync                                                                 u:object_r:asus_aura_device:s0
/dev/asus_rgb                                                                  u:object_r:asus_aura_device:s0
/dev/asus_lighting                                                             u:object_r:asus_aura_device:s0

# ROG Vision Display (Rear PMOLED)
/dev/asus_vision                                                               u:object_r:asus_vision_device:s0
/dev/rog_vision                                                                u:object_r:asus_vision_device:s0

# Gaming Haptic Feedback
/dev/asus_haptic                                                               u:object_r:asus_haptic_device:s0
/dev/gaming_vibrator                                                           u:object_r:asus_haptic_device:s0

