#####################################
# FLP (Fused Location Provider) Configuration
# ASUS ROG Phone 7 Fused Location Services
#####################################

# FLP capabilities
FLP_CAPABILITIES = GNSS, NETWORK, SENSORS

# FLP debug level
FLP_DEBUG_LEVEL = 2

# FLP operation mode
# 0: Low power, 1: Balanced, 2: High accuracy
FLP_OPERATION_MODE = 2

#####################################
# BATCHING CONFIGURATION
#####################################

# Location batching enabled
BATCHING_ENABLED = 1

# Maximum batch size (number of locations)
MAX_BATCH_SIZE = 1000

# Batching timeout (seconds)
BATCHING_TIMEOUT = 300

# Trip batching enabled
TRIP_BATCHING_ENABLED = 1

# Maximum trip batch size
MAX_TRIP_BATCH_SIZE = 100

#####################################
# GEOFENCING CONFIGURATION
#####################################

# Maximum number of geofences
