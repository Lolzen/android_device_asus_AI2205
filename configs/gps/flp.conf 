# Geofence confidence level
GEOFENCE_CONFIDENCE = 90

# Geofence dwell time (milliseconds)
GEOFENCE_DWELL_TIME = 30000

#####################################
# ACCURACY CONFIGURATION
#####################################

# High accuracy mode settings
