#####################################
# IZAT GENERAL CONFIGURATION
#####################################

# Log verbosity control for izat modules
# OFF = 0, ERROR = 1, WARNING = 2, INFO = 3, DEBUG = 4, VERBOSE = 5
IZAT_DEBUG_LEVEL = 2

# Select WIFI Wait Timeout value in seconds for SUPL
WIFI_WAIT_TIMEOUT = 10

#####################################
# NETWORK LOCATION CONFIGURATION
#####################################

# Select NLPI (Network Location Provider Interface) provider
# QTI default = 0
# OSNLP = 1
