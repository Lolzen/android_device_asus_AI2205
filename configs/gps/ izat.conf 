# Network location accuracy (in meters)
NLP_ACCURACY = 50

# Network location request timeout (in milliseconds)
NLP_TIMEOUT = 15000

#####################################
# GTP CONFIGURATION
#####################################

# GTP (Geofence Transition Processor) Mode
# GTP_MODE_DISABLED = 0
# GTP_MODE_LEGACY = 1
# GTP_MODE_SDK = 2
GTP_MODE = 2

# GTP response timeout (in seconds)
