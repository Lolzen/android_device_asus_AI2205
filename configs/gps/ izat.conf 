# Geofence engine mode
# GF_ENGINE_MODE_AUTO = 0
# GF_ENGINE_MODE_ALWAYS_ON = 1
GF_ENGINE_MODE = 0

# Geofence breach timeout (in seconds)
GF_BREACH_TIMEOUT = 30

#####################################
# PEDESTRIAN DEAD RECKONING
#####################################

# Enable pedestrian dead reckoning
# 0 = disable, 1 = enable
PEDESTRIAN_DEAD_RECKONING = 1

